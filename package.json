    "@iconify-json/ri": "^1.1.20",
    "@shikijs/rehype": "^1.3.0",
    "@unocss/reset": "^0.59.4",
    "astro": "^4.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "htmx.org": "^1.9.12",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "shiki": "^1.3.0",
    "solid-js": "^1.8.17",
    "ts-pattern": "^5.1.1",
    "unified": "^11.0.4"
