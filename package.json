{
  "name": "blog",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm codegen && astro check && astro build",
    "lint": "eslint src",
    "format": "prettier --write src",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "codegen": "pnpm codegen:graphql && pnpm codegen:workers",
