    "fast-glob": "^3.3.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "typescript": "^5.4.5",
    "unocss": "^0.59.4",
    "vite-plugin-checker": "^0.6.4",
    "wrangler": "^3.52.0"
  },
  "lint-staged": {
    "*.{astro,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.0.6"
}