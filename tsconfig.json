{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["@cloudflare/workers-types/experimental"]
  },
  "include": ["src/**/*"]
}
